    ArmorBar.BackgroundColor3 = Atlas['Target Aimbot']['Target UI'].Color or getgenv().targetuicolor
    HealthBar.BackgroundColor3 = Atlas['Target Aimbot']['Target UI'].Color or getgenv().targetuicolor
end)

